public class Main {
	public static void main(String[] args) {
		
		Sportscar car = new Sportscar();
		car.Start();
		car.Stop();
		car.Turn();
	}
}
public interface Idrivable {
	void Start();
	void Stop();
	void Turn();
}
public class Sportscar implements Idrivable {
	public void Start()
	{
		System.out.println("Start");
	}
	public void Stop()
	{
		System.out.println("Stop");
	}
	public void Turn()
	{
		System.out.println("Turn");
	}

}
